    <!-- Poids -->
    <FontWeight x:Key="Font.Weight.Normal">Normal</FontWeight>
    <FontWeight x:Key="Font.Weight.Medium">500</FontWeight>

    <!-- Tailles -->
    <x:Double x:Key="Text.Xs">12</x:Double>
    <x:Double x:Key="Text.Sm">14</x:Double>
    <x:Double x:Key="Text.Base">16</x:Double>
    <x:Double x:Key="Text.Lg">18</x:Double>
    <x:Double x:Key="Text.Xl">20</x:Double>
    <x:Double x:Key="Text.2Xl">24</x:Double>
    
    <!-- SideBar -->
    <x:Double x:Key="Sidebar.Width.Compact">50</x:Double>
    <x:Double x:Key="Sidebar.Width.Expanded">250</x:Double>
    <Thickness x:Key="Sidebar.DefaultPadding">16</Thickness>


    <ResourceDictionary.ThemeDictionaries>

        <!-- LIGHT -->
        <ResourceDictionary x:Key="Light">

            <!-- Font / radii -->
            <x:Double x:Key="Font.Size.Base">16</x:Double>
            <CornerRadius x:Key="Radius.Base">10</CornerRadius> <!-- 0.625rem ≈ 10px -->
            <CornerRadius x:Key="Radius.Sm">6</CornerRadius>
            <CornerRadius x:Key="Radius.Md">8</CornerRadius>
            <CornerRadius x:Key="Radius.Lg">10</CornerRadius>
            <CornerRadius x:Key="Radius.Xl">14</CornerRadius>

            <!-- Colors -->
            <Color x:Key="Color.Background">#FFFFFFFF</Color>
            <Color x:Key="Color.Foreground">#FF0B0B0B</Color>

            <Color x:Key="Color.Card">#FFFFFFFF</Color>
            <Color x:Key="Color.Card.Foreground">#FF0B0B0B</Color>

            <Color x:Key="Color.Popover">#FFFFFFFF</Color>
            <Color x:Key="Color.Popover.Foreground">#FF0B0B0B</Color>

            <Color x:Key="Color.Primary">#FF030213</Color>
            <Color x:Key="Color.Primary.Foreground">#FFFFFFFF</Color>

            <Color x:Key="Color.Secondary">#FFF1F2F7</Color>
            <Color x:Key="Color.Secondary.Foreground">#FF030213</Color>

            <Color x:Key="Color.Muted">#FFECECF0</Color>
            <Color x:Key="Color.Muted.Foreground">#FF717182</Color>

            <Color x:Key="Color.Accent">#FFE9EBEF</Color>
            <Color x:Key="Color.Accent.Foreground">#FF030213</Color>

            <Color x:Key="Color.Destructive">#FFD4183D</Color>
            <Color x:Key="Color.Destructive.Foreground">#FFFFFFFF</Color>

            <Color x:Key="Color.Border">#1A000000</Color> <!-- rgba(0,0,0,0.1) -->
            <Color x:Key="Color.Input">#00000000</Color> <!-- transparent -->
            <Color x:Key="Color.Input.Background">#FFF3F3F5</Color>
            <Color x:Key="Color.Switch.Background">#FFCBCED4</Color>

            <Color x:Key="Color.Ring">#FFB5B5B5</Color>

            <!-- Charts -->
            <Color x:Key="Color.Chart.1">#FFB45309</Color>
            <Color x:Key="Color.Chart.2">#FF0E7490</Color>
            <Color x:Key="Color.Chart.3">#FF1F3A5F</Color>
            <Color x:Key="Color.Chart.4">#FFEAB308</Color>
            <Color x:Key="Color.Chart.5">#FFF59E0B</Color>

            <!-- Sidebar -->
            <Color x:Key="Color.Sidebar">#FFF9FAFB</Color>
            <Color x:Key="Color.Sidebar.Foreground">#FF0B0B0B</Color>
            <Color x:Key="Color.Sidebar.Primary">#FF030213</Color>
            <Color x:Key="Color.Sidebar.Primary.Foreground">#FFF9FAFB</Color>
            <Color x:Key="Color.Sidebar.Accent">#FFF5F6F8</Color>
            <Color x:Key="Color.Sidebar.Accent.Foreground">#FF1A1A1A</Color>
            <Color x:Key="Color.Sidebar.Border">#FFE5E7EB</Color>
            <Color x:Key="Color.Sidebar.Ring">#FFB5B5B5</Color>

            <!-- Brushes (recommandé pour Setters) -->
            <SolidColorBrush x:Key="Brush.Background" Color="{DynamicResource Color.Background}" />
            <SolidColorBrush x:Key="Brush.Foreground" Color="{DynamicResource Color.Foreground}" />
            <SolidColorBrush x:Key="Brush.Card" Color="{DynamicResource Color.Card}" />
            <SolidColorBrush x:Key="Brush.Card.Foreground" Color="{DynamicResource Color.Card.Foreground}" />
            <SolidColorBrush x:Key="Brush.Primary" Color="{DynamicResource Color.Primary}" />
            <SolidColorBrush x:Key="Brush.Primary.Foreground" Color="{DynamicResource Color.Primary.Foreground}" />
            <SolidColorBrush x:Key="Brush.Secondary" Color="{DynamicResource Color.Secondary}" />
            <SolidColorBrush x:Key="Brush.Border" Color="{DynamicResource Color.Border}" />
            <SolidColorBrush x:Key="Brush.Input.Background" Color="{DynamicResource Color.Input.Background}" />
        </ResourceDictionary>

        <!-- DARK -->
        <ResourceDictionary x:Key="Dark">

            <x:Double x:Key="Font.Size.Base">16</x:Double>
            <CornerRadius x:Key="Radius.Base">10</CornerRadius>
            <CornerRadius x:Key="Radius.Sm">6</CornerRadius>
            <CornerRadius x:Key="Radius.Md">8</CornerRadius>
            <CornerRadius x:Key="Radius.Lg">10</CornerRadius>
            <CornerRadius x:Key="Radius.Xl">14</CornerRadius>

            <Color x:Key="Color.Background">#FF111111</Color>
            <Color x:Key="Color.Foreground">#FFF7F7F7</Color>

            <Color x:Key="Color.Card">#FF111111</Color>
            <Color x:Key="Color.Card.Foreground">#FFF7F7F7</Color>

            <Color x:Key="Color.Popover">#FF111111</Color>
            <Color x:Key="Color.Popover.Foreground">#FFF7F7F7</Color>

            <Color x:Key="Color.Primary">#FFF7F7F7</Color>
            <Color x:Key="Color.Primary.Foreground">#FF2A2A2A</Color>

            <Color x:Key="Color.Secondary">#FF2B2B2B</Color>
            <Color x:Key="Color.Secondary.Foreground">#FFF7F7F7</Color>

            <Color x:Key="Color.Muted">#FF2B2B2B</Color>
            <Color x:Key="Color.Muted.Foreground">#FFB5B5B5</Color>

            <Color x:Key="Color.Accent">#FF2B2B2B</Color>
            <Color x:Key="Color.Accent.Foreground">#FFF7F7F7</Color>

            <Color x:Key="Color.Destructive">#FF5A1F2B</Color>
            <Color x:Key="Color.Destructive.Foreground">#FFD94A4A</Color>

            <Color x:Key="Color.Border">#FF2B2B2B</Color>
            <Color x:Key="Color.Input">#FF2B2B2B</Color>
            <Color x:Key="Color.Ring">#FF707070</Color>

            <!-- Charts -->
            <Color x:Key="Color.Chart.1">#FF4F46E5</Color>
            <Color x:Key="Color.Chart.2">#FF10B981</Color>
            <Color x:Key="Color.Chart.3">#FFF59E0B</Color>
            <Color x:Key="Color.Chart.4">#FFA855F7</Color>
            <Color x:Key="Color.Chart.5">#FFEF4444</Color>

            <!-- Sidebar -->
            <Color x:Key="Color.Sidebar">#FF1A1A1A</Color>
            <Color x:Key="Color.Sidebar.Foreground">#FFF7F7F7</Color>
            <Color x:Key="Color.Sidebar.Primary">#FF4F46E5</Color>
            <Color x:Key="Color.Sidebar.Primary.Foreground">#FFF7F7F7</Color>
            <Color x:Key="Color.Sidebar.Accent">#FF2B2B2B</Color>
            <Color x:Key="Color.Sidebar.Accent.Foreground">#FFF7F7F7</Color>
            <Color x:Key="Color.Sidebar.Border">#FF2B2B2B</Color>
            <Color x:Key="Color.Sidebar.Ring">#FF707070</Color>

            <SolidColorBrush x:Key="Brush.Background" Color="{DynamicResource Color.Background}" />
            <SolidColorBrush x:Key="Brush.Foreground" Color="{DynamicResource Color.Foreground}" />
            <SolidColorBrush x:Key="Brush.Card" Color="{DynamicResource Color.Card}" />
            <SolidColorBrush x:Key="Brush.Card.Foreground" Color="{DynamicResource Color.Card.Foreground}" />
            <SolidColorBrush x:Key="Brush.Primary" Color="{DynamicResource Color.Primary}" />
            <SolidColorBrush x:Key="Brush.Primary.Foreground" Color="{DynamicResource Color.Primary.Foreground}" />
            <SolidColorBrush x:Key="Brush.Secondary" Color="{DynamicResource Color.Secondary}" />
            <SolidColorBrush x:Key="Brush.Border" Color="{DynamicResource Color.Border}" />
        </ResourceDictionary>

    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>